#include <iostream>
#include <string>
#include <fstream>
#include <vector>
#include <stdlib.h>
#include <stdio.h>
#include <sstream>
#include "job.h"

using namespace std;

int main()
{
  string text;
  ifstream infile("jobs.txt");
  vector<int> ids;
  vector<int> bursts;
  vector<string> records;
  char numbers[20];
  char buffer[10];
  vector<job> processes;
  int i = 0;

  if(infile.is_open()) {
	while(infile.good())
	  {
		getline(infile, text);
		records.push_back(text);
	  }
	  }
	else
	  {
		cerr << "Error opening file" << endl;
	  }
  if(records[0] != "\0")
  	{
  	  for(int j = 0; j < records.size(); j++)
  		{
  		  for(int k = 0; k < sizeof(records[i]) / sizeof(records[i][0]); k++)
			{
			  if(records[i][k] == ",")
				{
				  k = k+1;
				}
			}
  		}
  	}

  //cout << records[0][1] << endl;
  

  infile.close();
  return 0;
  }
